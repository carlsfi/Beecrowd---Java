import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        int fatorial = 1;
        
        for(int i = 1; i <=n ; i++){
            fatorial = i*fatorial;
        }System.out.println(fatorial);
    }
}
