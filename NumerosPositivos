import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) {
 
    Scanner dado = new Scanner(System.in);
    int pos = 0;
    for(int i = 0; i<6; i++){
    double x = dado.nextDouble();
    if(x>0){
        pos++;
    }
    }
    System.out.println(pos + " valores positivos");
    }
 
}
